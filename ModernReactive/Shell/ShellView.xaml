<mui:ModernWindow x:Class="ModernReactive.Shell.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ModernReactive"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        mc:Ignorable="d"
        ContentSource="../Intro/IntroView.xaml"
        Title="MainWindow" Height="450" Width="800">
    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="Menu 1">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="Intro" Source="../Intro/IntroView.xaml"/>
                <mui:Link DisplayName="Configs" Source="../Configs/ConfigsView.xaml"/>
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
    
</mui:ModernWindow>
